#include <stdio.h>

struct Animal
{
    fn() speak;
};

fn(void bark())
{
    printf("Arf!\n");
}

fn(void meow())
{
    printf("Meow.\n");
}

fn(int main())
{
    let dog : struct Animal;
    dog.speak = bark;

    let cat : struct Animal;
    cat.speak = meow;

    dog.speak();
    cat.speak();

    return 0;
}