#include <stdio.h> // printf

data Expr {
    Add { x: data Expr *, y: data Expr *, },
    Const { value: int, },
}

fn main() {
    let one: data Expr = Expr::Const { value: 1 };
    let two: data Expr = Expr::Const { value: 2 };
    let e: data Expr = Expr::Add { x: &one, y: &two };
    let Expr::Add { x, y } = e;
    let Expr::Const { xv } = x;
    let Expr::Const { yv } = y;
    printf("%d + %d\n", xv, yv);
    printf("working\n");
}
